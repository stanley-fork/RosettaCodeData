with javascript_semantics

function selection_sort(sequence s)
    for i,si in s do
        integer m = i
        object sm = si
        for j,sj in s from i+1 do
            if sj<sm then
                {sm,m} = {sj,j}
            end if
        end for
        if sm<si then -- (or equivalently m!=i)
            s[i] = sm
            s[m] = si
        end if
    end for
    return s
end function

?selection_sort(shuffle(tagset(10)))
