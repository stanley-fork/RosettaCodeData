import system'routines;
import extensions;

extension op
{
   (int, int) MinMax()
   {
      var ordered := self.ascendant();

      ^ (ordered.FirstMember, ordered.LastMember);
   }
}

public Program()
{
   var values := new int[]{4, 51, 1, -3, 3, 6, 8, 26, 2, 4};

   (int min, int max) := values.MinMax();

   Console.printLine("Min: ",min," Max: ",max)
}
