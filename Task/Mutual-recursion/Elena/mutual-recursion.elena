import extensions;
import system'collections;

F = (n => (n == 0) ? 1 : (n - M(F(n-1))) );
M = (n => (n == 0) ? 0 : (n - F(M(n-1))) );

public Program()
{
    var ra := new ArrayList();
    var rb := new ArrayList();

    for(int i := 0; i <= 19; i += 1)
    {
        ra.append(F(i));
        rb.append(M(i))
    };

    Console.printLine(ra.asEnumerable());
    Console.printLine(rb.asEnumerable())
}
