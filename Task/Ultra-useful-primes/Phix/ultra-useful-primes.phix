with javascript_semantics
atom t0 = time()
include mpfr.e
mpz p = mpz_init()

function a(integer n)
    mpz_ui_pow_ui(p,2,power(2,n))
    mpz_sub_si(p,p,1)
    integer k = 1
    while not mpz_prime(p) do
        k += 2
        mpz_sub_si(p,p,2)
    end while
    return k
end function

constant limit = iff(platform()=JS?10:
             iff(machine_bits()=32?12:13))
for i=1 to limit do
    printf(1,"%d ",a(i))
end for
?elapsed(time()-t0)
