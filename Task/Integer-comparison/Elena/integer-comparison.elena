import extensions;

public Program()
{
   var a := Console.readLine().toInt();
   var b := Console.readLine().toInt();

   if (a < b)
       { Console.printLine(a," is less than ",b) };

   if (a == b)
       { Console.printLine(a," equals ",b) };

   if (a > b)
       { Console.printLine(a," is greater than ",b) }
}
