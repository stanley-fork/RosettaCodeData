import extensions;

MakeList(separator)
{
    var counter := 1;

    var makeItem := (item){ var retVal := counter.toPrintable() + separator + item + NewLineConstant; counter += 1; ^ retVal };

    ^ makeItem("first") + makeItem("second") + makeItem("third")
}

public Program()
{
    Console.printLine(MakeList(". "))
}
