import system'routines;
import system'dynamic;
import extensions;

class MyClass
{
    int X    : prop;
    string Y : prop;
}

public Program()
{
    var o := new MyClass
    {
        this X := 2;

        this Y := "String";
    };

    MyClass.__getProperties().forEach::(p)
    {
        Console.printLine("o.",p,"=",p.getPropertyValue(o))
    }
}
