import system'dynamic;

class CameraFeature
{
    cameraMsg
        = "camera";
}

class MobilePhone
{
    mobileMsg
        = "phone";
}

singleton CameraPhone
{
   new() = new MobilePhone().mixInto(new CameraFeature());
}

public Program()
{
   var cp := CameraPhone.new();

   Console.writeLine(cp.cameraMsg);
   Console.writeLine(cp.mobileMsg)
}
