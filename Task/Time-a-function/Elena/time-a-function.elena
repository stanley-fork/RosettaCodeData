import system'calendar;
import system'routines;
import system'threading;
import system'math;
import extensions;

SomeProcess()
{
    Thread.sleep(1000);

    new Range(0,10000).filterBy::(x => x.mod(2) == 0).summarize();
}

public Program()
{
    var start := now;

    SomeProcess();

    var end := now;

    Console.printLine("Time elapsed in msec:",(end - start).Milliseconds)
}
