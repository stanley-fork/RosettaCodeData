singleton CameraFeature
{
    cameraMsg
        = "camera";
}

class MobilePhone
{
    mobileMsg
        = "phone";
}

class CameraPhone : MobilePhone
{
    dispatch() => CameraFeature;
}

public Program()
{
   var cp := new CameraPhone();

   Console.writeLine(cp.cameraMsg);
   Console.writeLine(cp.mobileMsg)
}
