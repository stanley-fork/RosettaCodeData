with javascript_semantics
function shoelace(sequence s)
    atom t = 0
    integer j = length(s)
    if j!=0 then
        sequence {x,y} = columnize(s)
        for i=1 to j do
            t += (y[j] + y[i]) * (x[j] - x[i])
            j = i
        end for
    end if
    return abs(t)/2
end function

constant test = {{3,4},{5,11},{12,8},{9,5},{5,6}}
?shoelace(test)
