with javascript_semantics
enum X, Y
function shoelace(sequence s)
    atom t = 0
    if length(s)>2 then
        s = append(deep_copy(s),s[1])
        for i=1 to length(s)-1 do
            t += s[i][X]*s[i+1][Y] - s[i+1][X]*s[i][Y]
        end for
    end if
    return abs(t)/2
end function

constant test = {{3,4},{5,11},{12,8},{9,5},{5,6}}
?shoelace(test)
