with javascript_semantics
include mpfr.e
function A(integer k)
    if k<5 then return mpz_init({1,0,-2,0,1}[k+1]) end if
    mpz c0 = mpz_init(3),
        c1 = mpz_init(2),
        c2 = mpz_init(1),
        c3 = mpz_init(0)
    for j=5 to k-1 do
        mpz_add(c3,c3,c0)
        mpz_sub_ui(c3,c3,1)
        mpz_add(c0,c0,c1)
        mpz_add(c1,c1,c2)
        mpz_add(c2,c2,c3)
    end for
    mpz_sub(c0,c0,c1)
    mpz_sub(c0,c0,c2)
    mpz_add(c0,c0,c3)
    return c0
end function

constant tests = tagset(20,0)&{30,39,500,10000,1e6}
for k=1 to length(tests)-(platform()=JS) do
    printf(1,"A(%,d) = %s\n", {tests[k],mpz_get_short_str(A(tests[k]))})
end for
