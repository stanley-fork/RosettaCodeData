with javascript_semantics

function gnomeSort(sequence s)
    integer i = 1, j = 2
    while i<length(s) do
        object si = s[i],
               sn = s[i+1]
        if si<=sn then
            i = j
            j += 1
        else
            s[i] = sn
            s[i+1] = si
            i -= 1
            if i=0 then
                i = j
                j += 1
            end if
        end if
    end while
    return s
end function

?gnomeSort(shuffle(tagset(10)))
