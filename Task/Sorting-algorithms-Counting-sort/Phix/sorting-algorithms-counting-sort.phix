with javascript_semantics

function counting_sort(sequence s, integer mine, maxe)
    sequence count = repeat(0,maxe-mine+1), res = {}
    for e in s do
        count[e-mine+1] += 1
    end for
    for i,c in count do
        res &= repeat(i+mine-1,c)
    end for
    return res
end function

sequence s = {5, 3, 1, 7, 4, 1, 1, 20}
?counting_sort(s,min(s),max(s))
