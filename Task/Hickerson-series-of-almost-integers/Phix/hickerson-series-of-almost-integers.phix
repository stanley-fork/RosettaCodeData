without javascript_semantics -- (no mpfr_log() for pwa/p2js)
requires("1.0.0") -- (mpfr_set_default_prec[ision] renamed)
include mpfr.e
mpfr_set_default_precision(-23) -- (found by trial/error)
mpfr ln2 = mpfr_init(2),
     hn = mpfr_init(0.5)
mpfr_log(ln2,ln2)
mpfr_div(hn,hn,ln2)
for n=1 to 17 do
    mpfr_mul_si(hn,hn,n)
    mpfr_div(hn,hn,ln2)
    string n10 = mpfr_sprintf("%24.4Rf ",hn)
    n10 &= iff(find(n10[21],"09")?"Y":"N")
    printf(1,"%2d:%s\n",{n,n10})
end for
